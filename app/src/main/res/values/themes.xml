<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ResolutionChecker"  parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">#ffd300</item>
        <item name="colorPrimaryDark">#ffd300</item>
        <item name="colorAccent">#ffd300</item>
        <item name="android:navigationBarColor">#181818</item>
        <item name="android:statusBarColor">#181818</item>
    </style>
    <style name="TextAppearance.AppCompat.Small.ResolutionChecker" parent="TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/google_sans_text</item>
    </style>

    <style name="TextAppearance.AppCompat.Small.ResolutionChecker.Medium" parent="TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/google_sans_text</item>
    </style>

    <style name="TextAppearance.AppCompat.Medium.ResolutionChecker" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/google_sans_text</item>

    </style>

    <style name="TextAppearance.AppCompat.Large.ResolutionChecker" parent="TextAppearance.AppCompat.Large">
        <item name="android:fontFamily">@font/google_sans_text</item>

    </style>

    <style name="TextAppearance.AppCompat.Large.OrderManager.Lighter" parent="TextAppearance.AppCompat.Large.ResolutionChecker">
        <item name="android:fontFamily">@font/google_sans_text</item>
    </style>
</resources>